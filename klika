from usermangement.models   import RequestFriend
from oauth.models           import User_info


# Get the logged-in user
logged_in_user = User_info.objects.get(username="new_username")

# Query all requests sent to this user that haven't been accepted
friend_requests = RequestFriend.objects.filter(to_user=logged_in_user, accepted=False)

# Print out the requests to confirm if they exist
for req in friend_requests:
    print(f"From: {req.from_user.username}, To: {req.to_user.username}, Accepted: {req.accepted}")
